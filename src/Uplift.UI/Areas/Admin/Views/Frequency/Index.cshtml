
@{
    ViewData["Title"] = "Lista de Frequencias";
}

<div class="border backgroundWhite">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">Lista de Frequencias</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Upsert" class="btn btn-primary">
                <i class="fas fa-plus"></i> &nbsp; Criar Nova Frequencia
            </a>
        </div>
    </div>
    <br />
    <br />
    <table id="tblData" class="table table-striped table-bordered" style="width: 100%">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Contagem de Frequencias</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts {
    <script src="~/js/frequency.js"></script>
}
